@model ModelBindingPractice.Models.Person

@{
    ViewBag.Title = "Index";
}

<h2>Person</h2>
@if (Model != null)
{
    <div>
        <label>Id:</label>
        @Html.DisplayFor(p => p.PersonId)
    </div>
    <div>
        <label>FirstName:</label>
        @Html.DisplayFor(p => p.FirstName)
    </div>
    <div>
        <label>LastName:</label>
        @Html.DisplayFor(p => p.LastName)
    </div>
    <div>
        <label>Role:</label>
        @Html.DisplayFor(p => p.Role)
    </div>
    <div>
        <label>City:</label>
        @Html.DisplayFor(p => p.HomeAddress.City)
    </div>
    <div>
        <label>Country:</label>
        @Html.DisplayFor(p => p.HomeAddress.Country)
    </div>
}

